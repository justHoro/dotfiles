#!/bin/bash
# 
# A script to set X11 bitmaps

### HELPER

_msg_prefix() {
	echo "[$(date +'%Y-%m-%dT%H:%M:%S%z')]: $*" >&1
}

err() {
	_msg_prefix "ERR $*" >&2 
}

info() {
	_msg_prefix "INFO $*"
}

### FUNCTIONS

update() {
	err test	
}

main() {
	update
}

feh --bg-fill --no-fehbg ~/.local/share/bg

#main $@
